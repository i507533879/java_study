
import java.util.Scanner;
public class sum {
    public static void main(String[] ages) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        long i,r=0;
        for (i=1;i<n;i++)
        {
            if(i%3==0||i%5==0){
                r+=i;
            }
        }
        System.out.println(r);
        input.close();
    }
}

